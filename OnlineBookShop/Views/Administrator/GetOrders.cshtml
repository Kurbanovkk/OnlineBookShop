@model List<Order>;
<div class="row">
    @await Html.PartialAsync("MenuAdministrator")

    <div class="col-9">
        <h3 class="text-center">Список заказов</h3>
        <div class="row">
            <div class="col">
                <table class="table">
                    <tbody>

                        @foreach (var order in Model)
                        {
                            <tr>
                                <td>@order.Cart.Cost</td>
                                <td>@order.Name</td>
                                <td>@order.Address</td>
                                <td>@order.PhoneNumber</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>